{
  "openapi": "3.0.1",
  "info": {
    "title": "Work API",
    "version": "1.0"
  },
  "paths": {
    "/work": {
      "post": {
        "summary": "Выполнение операции с параметрами наблюдения за потоком.",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "pisya_popa": {
                    "title": "asdasd",
                    "type": "string",
                    "format": "binary"
                  },
                  "pisya_popa_2": {
                    "type": "object",
                    "properties": {
                      "file": {
                        "type": "string",
                        "format": "binary"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Успешная операция."
          },
          "400": {
            "description": "Неверный запрос."
          }
        }
      }
    }
  }
}